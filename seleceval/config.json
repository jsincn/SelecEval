{
  "no_rounds": 5,
  "algorithm": [
    "ActiveFL",
    "FedCS",
    "random",
    "CEP",
    "PowD"
  ],
  "dataset": "mnist",
  "algorithm_config": {
    "ActiveFL": {
      "c": 0.2
    }
  },
  "data_config" : {
    "data_quantity_skew": "Uniform",
